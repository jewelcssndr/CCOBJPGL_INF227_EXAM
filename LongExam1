public class App {
    public static void main(String[] args) throws Exception {

        Locations boracay = new Boracay();
        Locations buscalan = new Buscalan();
        Locations cebu = new Cebu();
        Locations palawan = new Palawan();
        Locations siargao = new Siargao();

        Tourist tourist = new Me();


        boracay.accept(tourist);
        buscalan.accept(tourist);
        cebu.accept(tourist);
        palawan.accept(tourist);
        siargao.accept(tourist);
        
    }
}
